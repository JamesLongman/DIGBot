  },
  "homepage": "https://github.com/JamesLongman/DIGBot#readme",
  "dependencies": {
    "cli-color": "^1.4.0",
    "config": "^2.0.1",
    "discord.js": "~11.4.0",
    "googleapis": "^16.1.0",
    "lodash": "^4.17.11",
    "opusscript": "^0.0.6",
    "request": "^2.88.0",
    "usage": "^0.7.0",
    "ytdl-core": "^0.28.0"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.3",
    "erlpack": "discordapp/erlpack",
    "libsodium-wrappers": "^0.7.3",
