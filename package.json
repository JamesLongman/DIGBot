  "bugs": {
    "url": "https://github.com/JamesLongman/DIGBot/issues"
  },
  "homepage": "https://github.com/JamesLongman/DIGBot#readme",
  "dependencies": {
    "awilix": "^4.2.2",
    "axios": "^0.19.0",
    "bull": "~3.10.0",
    "config": "^3.2.2",
    "discord.js": "~11.5.1",
    "googleapis": "^42.0.0",
    "ioredis": "^4.14.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongoose": "^5.7.0",
    "opusscript": "^0.0.7",
    "os-utils": "^0.0.14",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0",
    "ytdl-core": "^0.29.5"
